{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*":["src/*"],
      "@Components/*":["src/components/*"],
      "@Views/*":["src/views/*"],
      "@UI/*":["src/components/UI/*"],
      "@API/*":["src/api/*"],
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "lib": ["dom", "es2015"],
    "jsx": "react"
  }
}
